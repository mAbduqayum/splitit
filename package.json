{
  "name": "splitit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "rm -rf .angular/ && ng serve --port 5544",
    "build": "ng build --verbose",
    "gh-build": "ng build --configuration=production --base-href=/splitit/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "biome:lint": "biome lint .",
    "biome:lint:write": "biome lint --write .",
    "biome:format": "biome format .",
    "biome:format:write": "biome format --write .",
    "biome:check": "biome check .",
    "biome:check:write": "biome check --write .",
    "clean": "rm -rf ./angular ./dist ./node_modules && echo 'done cleaning...'"
  },
  "dependencies": {
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/forms": "^20.1.6",
    "@angular/platform-browser": "^20.1.6",
    "@angular/router": "^20.1.6",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.6",
    "@biomejs/biome": "2.2.0",
    "typescript": "~5.8.3",
    "ultracite": "5.1.2"
  },
  "browserslist": {
    "production": [
      "last 5 chrome versions"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
