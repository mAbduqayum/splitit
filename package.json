{
  "name": "splitit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "rm -rf .angular/ && ng serve --port 5500",
    "build": "ng build --verbose",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "biome:lint": "biome lint .",
    "biome:lint:write": "biome lint --write .",
    "biome:format": "biome format .",
    "biome:format:write": "biome format --write .",
    "biome:check": "biome check .",
    "biome:check:write": "biome check --write .",
    "clean": "rm -rf ./angular ./dist ./node_modules && echo 'done cleaning...'"
  },
  "dependencies": {
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/forms": "^20.1.6",
    "@angular/platform-browser": "^20.1.6",
    "@angular/router": "^20.1.6",
    "@tailwindcss/postcss": "^4.1.11",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.2",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.6",
    "typescript": "~5.8.3"
  }
}
